{
  "version": 2,
  "builds": [
    {
      "src": "npm run build && npm run migrate && npm start",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "DATABASE_FILE": "database.sqlite",
    "DATABASE_URL": ""
  },
  "functions": {
    "api/*": {
      "memory": 512,
      "maxDuration": 10,
      "runtime": "@vercel/node"
    }
  }
}
