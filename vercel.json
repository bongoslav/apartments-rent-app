{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/app.js" }
  ],
  "env": {
    "NODE_ENV": "production",
    "DB_NAME": "database.sqlite",
    "DB_STORAGE": "file",
    "DB_DIALECT": "sqlite",
    "DB_LOGGING": "false"
  }
}
